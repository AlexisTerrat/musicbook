MusicalItem:
- id: uid
- userId: uid
- createdAt: Date
- updatedAt: Date
- artist: string (optional) [later]
- album: string (optional) [later]
- title: string (optional)
- live: boolean (optional) [later]
- links: Array<string> (must be valid link.id) (optional) [later]
- tags: Array<string> (must be valid tag.id) (optional)

Link: [later]
- id: uid
- itemId: uid
- type: string ('youtube' || 'soundcloud')
- url: string (valid http url)

Tag:
- id: uid
- userId: uid
- createdAt: Date
- updatedAt: Date
- name: string

Users:
- id: uid
- createdAt: Date
- updatedAt: Date
- name: string (unique)
- email: string [later]
- password: hashed & salted string [later]
- salt: string [later]

